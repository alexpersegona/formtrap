	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test:e2e": "playwright test",
		"test": "npm run test:unit -- --run",
		"db:push": "bunx --bun drizzle-kit push",
		"db:generate": "bunx --bun drizzle-kit generate",
		"db:migrate": "bunx --bun drizzle-kit migrate",
		"db:studio": "bunx --bun drizzle-kit studio",
		"email:dev": "email dev --dir emails --port 3001",
		"seed:submissions": "bun scripts/seed-submissions.ts",
		"seed:submissions:db": "bun scripts/seed-submissions-db.ts"
	},
